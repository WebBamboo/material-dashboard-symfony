<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('/bundles/materialdashboard/img/apple-icon.png') }}">
  <link rel="icon" type="image/png" href="{{ asset('/bundles/materialdashboard/img/favicon.png') }}">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <title>
    {% block meta_title %}Material Dashboard by Creative Tim{% endblock %}
  </title>
  <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
  {% block stylesheets %}
  <!--     Fonts and icons     -->
  <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
  <!-- CSS Files -->
  <link href="{{ asset('/bundles/materialdashboard/css/material-dashboard.css?v=2.1.1') }}" rel="stylesheet" />
  <!-- CSS Just for demo purpose, don't include it in your project -->
  <link href="{{ asset('/bundles/materialdashboard/demo/demo.css?v=2.1.1') }}" rel="stylesheet" />
  {% endblock %}
</head>

<body class="">
  <div class="wrapper ">
    <div class="sidebar" data-color="{{ color() }}" data-background-color="white" data-image="{{ sidebarBackground() }}">
      <!--
        Tip 1: You can change the color of the sidebar using: data-color="purple | azure | green | orange | danger"

        Tip 2: you can also add an image using data-image tag
    -->
      <div class="logo">
        <a href="{{ menuHeader().anchor }}" class="simple-text logo-normal">
          {{ menuHeader().title }}
        </a>
      </div>
      {% block sidebar %}
      <div class="sidebar-wrapper">
        <ul class="nav">
            {% for path, item in menuItems() %}
            <li class="nav-item{% if app.request.attributes.get('_route') == path %} active{% endif %}">
                <a class="nav-link" href="{{ getItemRoute(path, item) }}">
                <i class="material-icons">{{ item.icon }}</i>
                <p>{{ item.label }}</p>
                </a>
            </li>
            {% endfor %}
            {% if exampleMenu() %}
              {% if is_granted('ROLE_ADMIN') %}
              {% if app.request.attributes.get('_route') == 'user_index' or app.request.attributes.get('_route') == 'user_new' or app.request.attributes.get('_route') == 'user_show' or app.request.attributes.get('_route') == 'user_edit' %}
                {% set exampleMenuCollapsed = true %}
              {% else %}
                {% set exampleMenuCollapsed = false %}
              {% endif %}
              <li class="nav-item{{ exampleMenuCollapsed ? ' active' : ''}}">
                <a class="nav-link{{ exampleMenuCollapsed ? '' : ' collapsed'}}" data-toggle="collapse" href="#symfonyExample" aria-expanded="{{ exampleMenuCollapsed ? 'true' : 'false'}}">
                  <i><img style="width:25px" src="{{ asset('/bundles/materialdashboard/img/symfony.png') }}"></i>
                  <p>Symfony Examples
                    <b class="caret"></b>
                  </p>
                </a>
                <div class="{{ exampleMenuCollapsed ? 'collapse show' : 'collapse'}}" id="symfonyExample" style="">
                  <ul class="nav">
                    <li class="nav-item">
                      <a class="nav-link" href="{{ path('user_profile') }}">
                        <span class="sidebar-mini"> UP </span>
                        <span class="sidebar-normal">User profile </span>
                      </a>
                    </li>
                    <li class="nav-item {% if app.request.attributes.get('_route') == 'user_index' or app.request.attributes.get('_route') == 'user_new' or app.request.attributes.get('_route') == 'user_show' or app.request.attributes.get('_route') == 'user_edit' %} active{% endif %}">
                      <a class="nav-link" href="{{ path('user_index') }}">
                        <span class="sidebar-mini"> UM </span>
                        <span class="sidebar-normal"> User Management </span>
                      </a>
                    </li>
                  </ul>
                </div>
              </li>

              <li class="nav-item{% if app.request.attributes.get('_route') == 'example_table_list' %} active{% endif %}">
                <a class="nav-link" href="{{ path('example_table_list') }}">
                  <i class="material-icons">content_paste</i>
                    <p>Table List</p>
                </a>
              </li>
              <li class="nav-item{% if app.request.attributes.get('_route') == 'example_typography' %} active{% endif %}">
                <a class="nav-link" href="{{ path("example_typography") }}">
                  <i class="material-icons">library_books</i>
                    <p>Typography</p>
                </a>
              </li>
              <li class="nav-item{% if app.request.attributes.get('_route') == 'example_icons' %} active{% endif %}">
                <a class="nav-link" href="{{ path("example_icons") }}">
                  <i class="material-icons">bubble_chart</i>
                  <p>Icons</p>
                </a>
              </li>
              <li class="nav-item{% if app.request.attributes.get('_route') == 'example_maps' %} active{% endif %}">
                <a class="nav-link" href="{{ path("example_maps") }}">
                  <i class="material-icons">location_ons</i>
                    <p>Maps</p>
                </a>
              </li>
              <li class="nav-item{% if app.request.attributes.get('_route') == 'example_notifications' %} active{% endif %}">
                <a class="nav-link" href="{{ path("example_notifications") }}">
                  <i class="material-icons">notifications</i>
                  <p>Notifications</p>
                </a>
              </li>
              <li class="nav-item active-pro">
                <a class="nav-link" href="{{ path("example_upgrade") }}">
                  <i class="material-icons">unarchive</i>
                  <p>Upgrade to PRO</p>
                </a>
              </li>
              {% endif %}
            {% endif %}
        </ul>
      </div>
      {% endblock %}
    </div>
    <div class="main-panel">
      <!-- Navbar -->
      <nav class="navbar navbar-expand-lg navbar-transparent navbar-absolute fixed-top ">
        <div class="container-fluid">
          <div class="navbar-wrapper">
            <a class="navbar-brand" href="#pablo">{% block page_title %}Dashboard{% endblock %}</a>
          </div>
          <button class="navbar-toggler" type="button" data-toggle="collapse" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
            <span class="sr-only">Toggle navigation</span>
            <span class="navbar-toggler-icon icon-bar"></span>
            <span class="navbar-toggler-icon icon-bar"></span>
            <span class="navbar-toggler-icon icon-bar"></span>
          </button>
          <div class="collapse navbar-collapse justify-content-end">
                {% block search_bar %}
                <form class="navbar-form">
                    <div class="input-group no-border">
                        <input type="text" value="" class="form-control" placeholder="Search...">
                        <button type="submit" class="btn btn-white btn-round btn-just-icon">
                        <i class="material-icons">search</i>
                        <div class="ripple-container"></div>
                        </button>
                    </div>
                </form>
                {% endblock %}
            <ul class="navbar-nav">
              <li class="nav-item">
                <a class="nav-link" href="#pablo">
                  <i class="material-icons">dashboard</i>
                  <p class="d-lg-none d-md-block">
                    Stats
                  </p>
                </a>
              </li>
              {% block notifications %}
              <li class="nav-item dropdown" id="notificationsContainer">
                <a class="nav-link" href="http://example.com" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  <i class="material-icons">notifications</i>
                  <span class="notification">0</span>
                  <p class="d-lg-none d-md-block">
                    Notifications
                  </p>
                </a>
                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownMenuLink">
                </div>
              </li>
              {% endblock %}
              <li class="nav-item dropdown">
                <a class="nav-link" href="#pablo" id="navbarDropdownProfile" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  <i class="material-icons">person</i>
                  <p class="d-lg-none d-md-block">
                    Account
                  </p>
                </a>
                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownProfile">
                    {% for path, item in userMenuItems() %}
                        <a class="dropdown-item" href="{{ getItemRoute(path, item) }}">{{ item.label }}</a>
                    {% endfor %}
                </div>
              </li>
            </ul>
          </div>
        </div>
      </nav>
